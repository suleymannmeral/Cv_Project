
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h1>Add Service</h1>
<br />
<br />
<form method="post" class="mt-3">

    <div class="form-group">
        <label for="exampleInputEmail1">Skill Title</label>
        <input type="text" class="form-control" id="exampleInputEmail1" name="Title" placeholder="Service Title">
    </div>
   
    <div class="form-group">
        <label for="exampleInputPassword1">Icon URL</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="imageUrl" placeholder="Image URL">
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
</form>

