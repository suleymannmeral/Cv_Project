
@{
    Layout = null;
}
<style>
    .bacolor
    {
        background-color: #172B4D;
    }
</style>

<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("/Views/Admin/PartialHead.cshtml")
<body class="">
    @await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
    <div class="main-content">
        <!-- Navbar -->
        @await Html.PartialAsync("/Views/Admin/PartialNavbar.cshtml")
        <!-- End Navbar -->
        <!-- Header -->
        <div class="header bacolor pb-8 pt-5 pt-md-8">
            
        </div>
        <div class="container-fluid mt--7">
            <div class="row">
                <div class="col">       
                        @RenderBody()
                </div>
            </div>
            <!-- Footer -->
            @await Html.PartialAsync("/Views/Admin/PartialFooter.cshtml")
            <!-- Footer -->
            @await Html.PartialAsync("/Views/Admin/PartialScripts.cshtml")
        </div>
    </div>
</body>
</html>
