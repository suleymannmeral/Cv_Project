
@{
    ViewData["Title"] = "AddPortfolio";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<h1>Add Portfolio</h1>
<br />
<br />
<form method="post" class="mt-3">

    <div class="form-group">
        <label for="exampleInputEmail1">Project Title</label>
        <input type="text" class="form-control" id="exampleInputEmail1" name="Name" placeholder="Project Title">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Project Logo</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="ImageUrl" placeholder="Project Logo">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Budget</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="Budget" placeholder="Budget">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Completion</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="Completion" placeholder="Completion">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Image(1)</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="ImageUrl1" placeholder="ImageUrl1">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Image(2)</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="ImageUrl2" placeholder="ImageUrl2">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Project Platform Logo</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="ImageUrl3" placeholder="ImageUrl3">
    </div>
   
    <div class="form-group">
        <label for="exampleInputPassword1">Project Link URL</label>
        <input type="text" class="form-control" id="exampleInputPassword1" name="ProjectLink" placeholder="Project Link">
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
</form>
